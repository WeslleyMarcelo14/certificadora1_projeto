{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "src/components/login-remove/label.jsxsx",
    "src/components/login-remove/busrc/lib/menu-list-2.tsc/lib/menu-list.ts",
    "src/hooks/use-store.js",
    "src/hooks/use-sidebar.js",
    "src/app/auth/losrc/app/tarefas/page.jsx/categorsrc/app/tarefas/page.jsx/categories/pasrc/app/posts2/page.tsxposts/pasrc/app/posts2/new/page.tsxs/new/page.tsx",
    "src/app/dashboard/page.tsx",
    "src/app/tags/psrc/app/dashboard/page.jsxp/users/page.tsx",
    "src/app/auth",
    "src/components/ui/tooltip.jsx",
    "src/app/estoque/page.jsx",
    "src/components/ui/scroll-area.jsx",
    "src/components/ui/dropdown-menu.jsx",
    "src/components/ui/collapsible.jsx",
    "src/components/ui/card.jsx",
    "src/components/ui/button.jsx",
    "src/components/ui/breadcrumb.jsx",
    "src/components/ui/avatar.jsx",
    "src/components/screens/placeholder.jsx",
    "src/components/dashboard/user-navbar.jsx",
    "src/components/dashboard/sidebar-toggle.jsx",
    "src/components/dashboard/sheet-menu.jsx",
    "src/components/dashboard/footer.jsx",
    "src/components/dashboard/content-layout.jsx",
    "src/components/dashboard/collapse-menu-button.jsx",
    "src/app/providers.jsx",
    "src/components/dashboard/navbar.jsx",
    "/login/layout.tsx",
    "src/app/api/auth/[...nextauth]/route.js",
    "src/lib/utils.js",
    "src/app/usuarios/page.jsx"
  ],
  "exclude": ["node_modules"]
}
